{
  "name": "al3x2508/easypiba",
  "description": "A lightweight PHP framework, easy to install, easy to learn, easy to develop.",
  "keywords": [
    "framework",
    "easypiba"
  ],
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Alexandru Glavan",
      "email": "alexandru.glavan@easypiba.com"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "php": ">=7.4",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-zip": "*",
    "tedivm/jshrink": "*",
    "phpoffice/phpspreadsheet": "*",
    "masterexploder/phpthumb": "*",
    "phpmailer/phpmailer": "*",
    "symfony/dotenv": "^5.4",
    "gettext/gettext": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Controller\\": "Controller/",
      "Model\\": "Model/",
      "Module\\": "Module/",
      "Utils\\": "Utils/",
      "\\": "Utils/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "9.5"
  }
}
